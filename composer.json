{
    "name": "yiisoft/mutex",
    "type": "library",
    "description": "Yii Mutex Library",
    "keywords": [
        "yii",
        "mutex"
    ],
    "homepage": "http://www.yiiframework.com/",
    "license": "BSD-3-Clause",
    "support": {
        "source": "https://github.com/yiisoft/mutex",
        "issues": "https://github.com/yiisoft/mutex/issues",
        "forum": "http://www.yiiframework.com/forum/",
        "wiki": "http://www.yiiframework.com/wiki/",
        "irc": "irc://irc.freenode.net/yii"
    },
    "require": {
        "php": "^7.4"
    },
    "require-dev": {
        "phpunit/phpunit": "^9.0",
        "squizlabs/php_codesniffer": "^3.2"
    },
    "suggest": {
        "yiisoft/mutex-db-mysql": "version ^3.0 to use the MySQL driver for mutex",
        "yiisoft/mutex-db-oracle": "version ^3.0 to use the Oracle driver for mutex",
        "yiisoft/mutex-db-pgsql": "version ^3.0 to use the Postgres driver for mutex",
        "yiisoft/mutex-db-redis": "version ^3.0 to use the Redis driver for mutex",
        "yiisoft/mutex-file": "version ^3.0 to use the File driver for mutex"
    },
    "autoload": {
        "psr-4": {
            "Yiisoft\\Mutex\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Yiisoft\\Mutex\\Tests\\": "tests"
        }
    },
    "extra": {
        "branch-alias": {
            "dev-master": "3.0.x-dev"
        }
    },
    "config": {
        "sort-packages": true
    }
}
